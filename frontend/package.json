{
  "name": "elevated-compliance-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-types": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production tsc -b && vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:debug": "vitest --inspect-brk --no-coverage --reporter=verbose",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:visual": "vitest --grep visual",
    "test:visual:update": "vitest --grep visual --updateSnapshot",
    "test:a11y": "vitest --grep accessibility",
    "test:a11y:report": "vitest --grep accessibility --reporter=html",
    "test:performance": "vitest --grep performance",
    "test:performance:industrial": "vitest --grep 'performance.*industrial'",
    "test:performance:report": "vitest --grep performance --reporter=html",
    "test:all": "npm run test:coverage && npm run test:e2e && npm run test:a11y && npm run test:performance",
    "test:ci": "npm run test:coverage -- --run && npm run test:e2e -- --headless",
    "cypress:install": "cypress install",
    "cypress:verify": "cypress verify",
    "build:vercel": "vite build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.16",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.9.2",
    "@mui/x-tree-view": "^8.9.2",
    "@tanstack/react-query": "^5.84.1",
    "@types/leaflet": "^1.9.20",
    "@types/papaparse": "^5.3.16",
    "@types/socket.io-client": "^1.4.36",
    "@vercel/blob": "^1.1.1",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "fuse.js": "^7.1.0",
    "jsqr": "^1.4.0",
    "leaflet": "^1.9.4",
    "papaparse": "^5.5.3",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.7.1",
    "react-swipeable": "^7.0.2",
    "recharts": "^3.1.0",
    "socket.io-client": "^4.8.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cypress": "^14.5.3",
    "cypress-axe": "^1.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^4.0.0",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0"
  }
}
